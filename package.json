  "jest": {
    "coverageDirectory": "<rootDir>/target/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/config/tests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
